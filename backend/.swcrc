{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true,
      "decorators": true
    },
    "transform": {
       "legacyDecorator": true,
       "decoratorMetadata": true
    },
    "target": "es2020",
    "loose": true,
    "externalHelpers": false,
    "keepClassNames": true
  },
  "module": {
      "type": "commonjs",
      "strict": false,
      "strictMode": false,
      "lazy": false,
      "noInterop": false
    },
  "minify": false
}